        },
        "defaultQuantity": {
          "type": "integer"
        },
        "entityType": {
          "type": "string"
        },
        "isNavigable": {
          "type": "boolean"
        },
        "isSource": {
          "type": "boolean"
        },
        "maxQuantity": {
          "type": "integer"
        },
        "minQuantity": {
          "type": "integer"
        },
        "role": {
          "type": "string"
        }
      },
      "allOf": [
        {
          "$ref": "../Common/Entity.schema.json#Entity"
        }
      ]
