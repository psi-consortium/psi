                "value": {
                    "type": "object",
                    "description": "Value of the characteristic"
                }
            },
            "allOf": [
                {
                    "$ref": "CharacteristicValueSpecification.schema.json#CharacteristicValueSpecification"
                }
            ]
        }
    }
}