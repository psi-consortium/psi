{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "OrderStateType.schema.json",
  "title": "OrderStateType",
  "definitions": {
    "OrderStateType": {
      "$id": "#OrderStateType",
      "type": "string",
      "description": "Possible values for the state of the order",
      "enum": [
        "acknowledged",
        "rejected",
        "pending",
        "held",
        "inProgress",
        "cancelled",
        "completed",
        "failed",
        "partial",
        "assessingCancellation",
        "pendingCancellation"
      ]
    }
  }
}
