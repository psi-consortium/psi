{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "IOTPropertyAnalytics.schema.json",
    "title": "IOTPropertyAnalytics",
    "definitions": {
        "IOTPropertyAnalytics": {
            "$id": "#IOTPropertyAnalytics",
            "type": "object",
            "properties": {
                "dataCreationTimestamp": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Time stamp for data creation (e.g. system dump creation, event generation\u2026)"
                },
                "runTimestamp": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Time stamp for data upload run"
                },
                "validFromTimestamp": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Time stamp for business validity of entity record"
                }
            },
  
            "allOf": [{"$ref": "../Common/Entity.schema.json#Extensible"}]
        }
    }
}